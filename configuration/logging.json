{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "standard": {
      "format": "%(asctime)s %(levelname)s [ %(filename)s:%(lineno)s - %(funcName)s() ] - %(message)s"
    }
  },
  "handlers": {
    "default": {
      "level": "DEBUG",
      "formatter": "standard",
      "class": "logging.StreamHandler",
      "stream": "ext://sys.stdout"
    }
  },
  "loggers": {
    "root": {
      "handlers": [
        "default"
      ],
      "level": "DEBUG",
      "propagate": true
    },
    "discord": {
      "handlers": [
        "default"
      ],
      "level": "DEBUG",
      "propagate": true
    }
  }
}
